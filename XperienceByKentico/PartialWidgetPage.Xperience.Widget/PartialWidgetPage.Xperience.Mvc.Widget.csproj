<Project Sdk="Microsoft.NET.Sdk.Razor">

	<PropertyGroup>
		<TargetFramework>net6.0</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>disable</Nullable>
		<AddRazorSupportForMvc>true</AddRazorSupportForMvc>
		<RootNamespace>PartialWidgetPage</RootNamespace>
		<LangVersion>latest</LangVersion>
		<GeneratePackageOnBuild>True</GeneratePackageOnBuild>

		<Version>28.0.0</Version>
		<Authors></Authors>
		<Company></Company>
		<Product>Partial Widget Page</Product>
		<Description>For Xperience by Kentico .net Core only, provides tag helpers and interfaces to be able to render other pages (with widgets) in your page.  Useful for things like Mega Menus, editable header/footers, Tabs, accordions, etc.</Description>
		<Copyright></Copyright>
		<PackageProjectUrl>https://github.com/KenticoDevTrev/PartialWidgetPage</PackageProjectUrl>
		<PackageIcon>kentico.png</PackageIcon>
		<RepositoryUrl>https://github.com/KenticoDevTrev/PartialWidgetPage</RepositoryUrl>
		<RepositoryType>GIT</RepositoryType>
		<PackageTags>Xperience By Kentico, Page Builder, Widgets</PackageTags>
		<PackageReleaseNotes>Adding &lt;vc:render-page document-id="" /&gt; and &lt;vc:render-page-optimized typed-page=@MyTypedTreeNode /&gt; View Component that resolves the inline page using normal kentico routing, including Page Template support.</PackageReleaseNotes>

	</PropertyGroup>

	<ItemGroup>
		<FrameworkReference Include="Microsoft.AspNetCore.App" />
	</ItemGroup>

	<ItemGroup>
		<AssemblyAttribute Include="CMS.AssemblyDiscoverableAttribute">
		</AssemblyAttribute>
	</ItemGroup>

	<ItemGroup>
		<PackageReference Include="kentico.xperience.webapp" />

	</ItemGroup>

	<ItemGroup>
	  <ProjectReference Include="$(ProjectDir)..\PartialWidgetPage.Xperience.Mvc.Core\PartialWidgetPage.Xperience.Mvc.Core.csproj" />
	</ItemGroup>

	<ItemGroup>
		<None Include="$(SolutionDir)\kentico.png">
			<Pack>True</Pack>
			<PackagePath></PackagePath>
		</None>
	</ItemGroup>

</Project>
